    "@distube/spotify": "^1.5.1",
    "@distube/yt-dlp": "^1.1.3",
    "@distube/ytdl-core": "^4.11.10",
    "@distube/ytsr": "^2.0.0",
    "discord.js": "^14.11.0",
    "distube": "^4.1.1",
    "dotenv": "^16.1.4",
    "express": "^4.18.1",
    "ffmpeg-static": "^5.1.0",
    "fs": "^0.0.1-security",
