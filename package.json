{
  "name": "sicbot",
  "version": "3.0.0",
  "description": "Discord music bot built using discord.js and distube",
  "main": "main.js",
  "scripts": {
    "test": "nodemon main.js",
    "start": "node main.js",
    "delete:guild": "node delete/guild.js",
    "delete:global": "node delete/global.js",
    "format": "prettier --write .",
    "lint": "eslint ./",
    "lint:fix": "npm run lint -- --fix"
